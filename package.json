{
  "name": "FUA",
  "displayName": "FindUnusedAssets",
  "description": "Find & Remove Your React Project's Unused Assets",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.84.0"
  },
  "icon": "./fua_icon.png",
  "repository": {"url": "https://github.com/durmazoguzhan/FUA.git"},
  "publisher": "OguzhanDurmaz",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "FUA.findUnusedAssets",
        "title": "Find Unused Assets"
      },
      {
        "command": "FUA.removeUnusedAssets",
        "title": "Remove Unused Assets"
      },
      {
        "command": "FUA.removeUnusedReactFiles",
        "title": "Remove Unused React Files"
      },
      {
        "command": "FUA.removeAllUnusedFiles",
        "title": "Remove All Unused Files"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}
